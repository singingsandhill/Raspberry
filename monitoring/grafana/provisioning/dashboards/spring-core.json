    {
      "type": "singlestat",
      "title": "Heap Used (%)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "format": "percent",
      "valueName": "current",
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{application=\"$application\", instance=\"$instance\", area=\"heap\"}) * 100 / sum(jvm_memory_max_bytes{application=\"$application\", instance=\"$instance\", area=\"heap\"})",
          "refId": "A"
        }
      ],
      "thresholds": "70,90",
      "colorValue": true,
      "gauge": {
        "show": true
      },
      "decimals": 1,
      "valueFontSize": "80%",
      "id": 2
    },
    {
      "type": "graph",
